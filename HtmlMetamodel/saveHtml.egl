[% 
	var a:Template:=TemplateFactory.load('GeneratePromocionHtml.egl');
	var d:Integer;
	var c:Sequence;
	d=0;
	
	c=a.process().split('split--');
	c[0].println();
	while(d< c.size-1){
	
	var e:Template:=TemplateFactory.prepare(c[d].toString());
	e.generate('Promociones/Promociones'+d+'.html');
	d=d+1;
	}
	
	
	
	
	
%]